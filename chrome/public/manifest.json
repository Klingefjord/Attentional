{    
	"name": "Attentional â€“ Hide distracting content",    
	"version": "0.0.1.0",    
	"description": "Hide distracting content.",    
	"manifest_version": 3,
	"permissions": [
    "scripting",
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
    "icons": {
      "16": "icons/logo-16.png",
      "32": "icons/logo-32.png",
      "48": "icons/logo-48.png",
      "64": "icons/logo-64.png",
      "128": "icons/logo-128.png"
    },
	"options_page": "options.html",
	"background": {  
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_icon": "icons/logo-48.png"
  },
  "content_scripts": [{
    "css": [ "css/main.css" ],
    "matches": [ "<all_urls>" ],
    "js": [
      "featureRemover.bundle.js"
    ]
  }],
  "web_accessible_resources": [{
    "resources": [
      "css/main.css",
      "index.html"

    ],
    "matches": ["<all_urls>"]
  }],
  "content_security_policy": {
    "script-src": "self",
    "object-src":  "self"
  }
}
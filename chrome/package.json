{
  "name": "chrome-extension-webpack",
  "version": "0.0.1",
  "description": "A boilerplate to chrome extension with webpack",
  "scripts": {
    "build": "node utils/build.js",
    "dev": "NODE_ENV=development webpack --mode development --config webpack.config.js --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "fs-extra": "8.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.9.0",
    "webpack-extension-manifest-plugin": "^0.5.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator"
    ]
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-input-slider": "^6.0.0",
    "react-onclickoutside": "^6.9.0",
    "regenerator-runtime": "^0.13.7"
  }
}

{    
	"name": "Attentional",    
	"version": "0.1",    
	"description": "Control your attention",    
	"manifest_version": 2,
	"permissions": [
    "activeTab", 
    "declarativeContent", 
    "storage", 
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/"],
	"options_page": "options.html",
	"background": {  
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon-32.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

